def detect_vortex_cores(phase_field, min_circulation=None):
    """
    Detect vortex cores with Bayesian priors on charge stability.
    Parameters:
        min_circulation: Auto-set via noise level if None
    """
    grad_magnitude = np.sqrt(np.sum(np.gradient(phase_field)**2, axis=0))
    local_minima = find_local_minima(grad_magnitude)
    min_circulation = estimate_noise_circulation(phase_field) if min_circulation is None else min_circulation
    vortex_cores = []
    for point in local_minima:
        charge, circulation = compute_topological_charge(phase_field, point)
        if abs(circulation) &gt; min_circulation and abs(charge) &gt;= 1:
            vortex_cores.append({
                'position': point,
                'charge': charge,
                'circulation': circulation,
                'strength': abs(circulation)
            })
    return cluster_nearby_vortices(vortex_cores, max_dist=estimate_vortex_spacing(phase_field))

