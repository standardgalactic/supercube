def robust_phase_unwrap(phase_data, method='goldstein', noise_threshold=0.5):
    """
    Spatial phase unwrapping with adaptive noise handling.
    - Goldstein branch-cut (default)
    - Quality-guided path following
    - Minimum norm with noise regularization
    Parameters:
        noise_threshold: Max acceptable phase variance (default: 0.5 radians)
    """
    phase_variance = estimate_phase_noise(phase_data)
    if phase_variance &gt; noise_threshold:
        method = 'minimum_norm'  # Switch to robust method for noisy data
    if method == 'goldstein':
        return goldstein_unwrap(phase_data)
    elif method == 'quality_guided':
        return quality_guided_unwrap(phase_data)
    else:
        return min_norm_unwrap(phase_data, reg_lambda=0.1)

Multi-Scale Gradient Estimation
def multiscale_phase_gradient(phase_map, scales=[1, 3, 5], coherence_metric='plv'):
    """
    Compute phase gradients at multiple scales, weighted by phase-locking value (PLV).
    Parameters:
        coherence_metric: 'plv' (phase-locking value) or 'mi' (mutual information)
    """
    gradients, weights = [], []
    for scale in scales:
        smoothed_phase = gaussian_filter(phase_map, sigma=scale)
        grad = np.gradient(smoothed_phase)
        coherence = compute_phase_coherence(smoothed_phase, scale, metric=coherence_metric)
        gradients.append(grad)
        weights.append(coherence)
    return weighted_average(gradients, weights)

